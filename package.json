  },
  "keywords": [
    "reserve",
    "cookie"
  ],
  "author": "Arnaud Buchholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArnaudBuchholz/reserve-biscuit/issues"
  },
  "homepage": "https://github.com/ArnaudBuchholz/reserve-biscuit#readme",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.0.1",
    "reserve": "^1.7.1",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "cookie": "^0.4.1"
  },
  "standard": {
    "globals": [
      "process",
      "describe",
      "it",
      "before",
      "after"
    ]
  },
